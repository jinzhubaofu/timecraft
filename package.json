{
  "name": "timecraft",
  "version": "1.0.0",
  "description": "a hackable markdown note",
  "scripts": {
    "setup": "BUILDING_DLL=true webpack --display-chunks --color --config tools/webpack.dll.js",
    "start": "NODE_ENV=development node tools/server.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "env": {
      "production": {
        "only": [
          "app"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      },
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "license": "MIT",
  "dependences": {},
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "postcss-css-reset": "^1.0.2",
    "postcss-cssnext": "^2.8.0",
    "postcss-focus": "^1.0.0",
    "postcss-loader": "^0.13.0",
    "postcss-reporter": "^1.4.1",
    "pre-commit": "^1.1.3",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "text-loader": "0.0.1"
  },
  "dependencies": {
    "codemirror": "^5.19.0",
    "events": "^1.1.1",
    "github-markdown-css": "^2.4.1",
    "highlight.js": "^9.7.0",
    "markdown-it": "^8.0.0",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^2.5.0",
    "markdown-it-container": "^2.0.0",
    "markdown-it-deflist": "^2.0.1",
    "markdown-it-emoji": "^1.2.0",
    "markdown-it-footnote": "^3.0.1",
    "markdown-it-ins": "^2.0.0",
    "markdown-it-mark": "^2.0.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "react-addons-update": "^15.3.2"
  }
}
